{
  "name": "ai-video-storyboard-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "proxy": "node scripts/gemini-proxy.mjs",
    "debug:gemini:upload": "node scripts/gemini-debug.mjs upload sample.mp4 video/mp4 --verbose",
    "debug:gemini:analyze-video": "node scripts/gemini-debug.mjs analyze-video sample.mp4 video/mp4 gemini-2.5-flash --verbose",
    "debug:gemini:analyze-image": "node scripts/gemini-debug.mjs analyze-image sample.jpg image/jpeg gemini-2.5-flash --verbose"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^6.1.6",
    "react-markdown": "^9.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
